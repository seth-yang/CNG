<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"> 
    <item android:state_pressed="true"> 
        <shape>
            <solid android:color="#f6a407" />
            <stroke
                android:width="1dip" 
                android:color="#FFCB88" /> 
            <corners 
                android:radius="5dp" />
            <padding
                    android:left="20dp"
                    android:top="10dp"
                    android:right="20dp"
                    android:bottom="10dp" />
        </shape>
    </item> 
  
    <item android:state_focused="true"> 
        <shape>
            <solid android:color="#f6a407" />
            <stroke 
                android:width="1dip" 
                android:color="#BBD5F4" /> 
            <corners 
                android:radius="5dp" />
            <padding
                    android:left="20dp"
                    android:top="10dp"
                    android:right="20dp"
                    android:bottom="10dp" />
        </shape>
    </item> 
  
    <item> 
        <shape>
            <solid android:color="#f6a407" />
            <stroke 
                android:width="1dip" 
                android:color="#d58700" />
            <corners 
                android:radius="5dip" /> 
            <padding
                android:left="20dp"
                android:top="10dp"
                android:right="20dp"
                android:bottom="10dp" />
        </shape>
    </item> 
</selector>